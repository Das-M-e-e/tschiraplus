<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Global Colors -->
    <SolidColorBrush x:Key="Background" Color="#131921" />
    <SolidColorBrush x:Key="DividerColor" Color="#3d444d" />
    <SolidColorBrush x:Key="Foreground" Color="White" />
    <SolidColorBrush x:Key="BarBackground" Color="#0d1117" />
    <SolidColorBrush x:Key="LinksForeground" Color="#4493f8" />
    <SolidColorBrush x:Key="LinksBackground" Color="Transparent" />
    
    <!-- StandardStyle Colors-->
    <SolidColorBrush x:Key="TextBoxBackground" Color="Gray" />
    <SolidColorBrush x:Key="ButtonBackground" Color="#1f6feb" />
